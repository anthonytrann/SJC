<body data-gr-c-s-loaded="true">

  <% include navbar.ejs %>

  <div class="jumbotron">

    <h1>Add Companion</h1>

    <form method="post">
      <div class="col-md-12">
        <label for="inputCompanion" hidden>Add Companion</label>
        <div class="form-group" id="inputCompanion">
          <div class="form-row">
            <div class="form-group col-md-4">
              <label for="inputCompanion">Companion Name</label>
              <input type="text" class="form-control" id="inputCompanionName" name="companionName" required>
            </div>
            <div class="form-group col-md-4">
              <label for="inputCompanionType">Companion Type</label>
              <select id="inputCompanionType" class="form-control" name="companionType" required>
                <option selected disabled>Please select..</option>
                <option>Formator</option>
                <option>Spiritual Director</option>
              </select>
            </div>
          </div>
        </div>
      </div>
      <div class="text-center">
        <button type="submit" class="btn btn-primary">Add</button>
      </div>
    </form>


    <script src="https://code.jquery.com/jquery-3.4.1.min.js"
      integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
