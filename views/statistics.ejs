<body data-gr-c-s-loaded="true">


  <%- include navbar.ejs %>

  <h1>Statistics</h1>

  <div class="ct-chart .ct-perfect-fourth" id="chart1"></div>
  <div class="ct-chart .ct-perfect-fourth" id="chart2"></div>
 <% let comRec = communityRecords %>
 

  <script>
    var data1 = {
      labels: ['Bananas', 'Apples', 'Grapes'],
      series: [20, 15, 40]
    }; var data2 = {
      labels: ['Bananas', 'Apples', 'Grapes'],
      series: [[20, 15, 40]]
    };

    let comRecords = comRec

    var options = {
      width: 450,
      height: 200,
      // labelInterpolationFnc: function (value) {
      //   return value[0].
      // }
    };

    var responsiveOptions = [
      ['screen and (min-width: 640px)', {
        chartPadding: 30,
        labelOffset: 100,
        labelDirection: 'explode',
        labelInterpolationFnc: function (value) {
          return value;
        }
      }],
      ['screen and (min-width: 1024px)', {
        labelOffset: 80,
        chartPadding: 20
      }]
    ];

    new Chartist.Pie('#chart1', data1, options, responsiveOptions);
    new Chartist.Line('#chart2', data2, options);
  </script>


